{
  "manifest_version": 2,

  "name": "Hello World",
  "description": "Say hello.",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [{
    "js": ["jquery-2.0.3.min.js","front.js"],
    "css": ["style.css"],
    "matches": ["http://*/*", "https://*/*"]
  }],

  "permissions": [
    "tabs",
    "http://*/*"
  ],

"web_accessible_resources": [
    "style.css"
]

}